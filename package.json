		"postcss-import-ext-glob": "^2.1.1",
		"postcss-load-config": "^4.0.1",
		"postcss-preset-env": "^9.1.3",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prisma": "^5.2.0",
		"satori": "^0.10.4",
		"satori-html": "^0.3.2",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vitest": "^0.34.4"
	},
	"type": "module",
	"engines": {
		"node": ">=18.0.0 <19.0.0 || =20.5.0",
